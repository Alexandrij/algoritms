{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-shadowed-variable": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "triple-equals": [
      false,
      "allow-null-check"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-console": [
      true,
      "warn",
      "error"
    ],
    "arrow-parens": false,
    "semicolon": false,
    "space-before-function-paren": false
  }
}
