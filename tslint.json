{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "variable-name": false,
        "no-shadowed-variable": false,
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ]
    }
}